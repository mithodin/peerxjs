  "author": "Lucas L. Treffenst√§dt",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mithodin/peerxjs/issues"
  },
  "homepage": "https://github.com/mithodin/peerxjs#readme",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.1",
